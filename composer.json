{
  "name": "srag/h5pcron",
  "description": "Clean old data from H5P contents that is no longer required",
  "version": "1.2.3",
  "extra": {
    "ilias_plugin": {
      "id": "h5pcron",
      "name": "H5PCron",
      "ilias_min_version": "5.4.0",
      "ilias_max_version": "6.999",
      "slot": "Services/Cron/CronHook",
      "supports_cli_setup": true
    },
    "generate_plugin_readme_template": "SRAG_ILIAS_PLUGIN",
    "long_description_template": "src/LONG_DESCRIPTION.md"
  },
  "type": "project",
  "keywords": [
    "cron",
    "h5p",
    "ilias",
    "job",
    "plugin"
  ],
  "homepage": "https://github.com/studer-raimann/H5PCron",
  "license": "GPL-3.0-only",
  "authors": [
    {
      "name": "studer + raimann ag - Team Custom 1",
      "email": "support-custom1@studer-raimann.ch",
      "homepage": "https://studer-raimann.ch",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://plugins.studer-raimann.ch/goto.php?target=uihk_srsu_PLH5P"
  },
  "require": {
    "php": ">=7.0",
    "srag/generateplugininfoshelper": ">=0.1.0",
    "srag/librariesnamespacechanger": ">=0.1.0"
  },
  "autoload": {
    "psr-4": {
      "srag\\Plugins\\H5PCron\\": "src/"
    },
    "classmap": [
      "classes/"
    ],
    "files": [
      "../../../Repository/RepositoryObject/H5P/vendor/autoload.php"
    ]
  },
  "config": {
    "classmap-authoritative": true,
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "scripts": {
    "pre-autoload-dump": [
      "srag\\LibrariesNamespaceChanger\\LibrariesNamespaceChanger::rewriteLibrariesNamespaces",
      "srag\\LibrariesNamespaceChanger\\PHP72Backport::PHP72Backport",
      "srag\\GeneratePluginInfosHelper\\H5PCron\\GeneratePluginPhpAndXml::generatePluginPhpAndXml",
      "srag\\GeneratePluginInfosHelper\\H5PCron\\GeneratePluginReadme::generatePluginReadme"
    ]
  }
}
